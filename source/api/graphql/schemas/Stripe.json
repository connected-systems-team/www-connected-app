{
    "types": {
        "input": {
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "Float": {
                "kind": "scalar",
                "type": "Float",
                "description": "The `Float` scalar type represents signed double-precision fractional values as specified by [IEEE 754](https://en.wikipedia.org/wiki/IEEE_floating_point)."
            },
            "StripeMeteredEventLogStatus": {
                "kind": "enum",
                "type": "StripeMeteredEventLogStatus",
                "description": "The status of the Stripe metered event log.",
                "values": ["Pending", "Success", "Failed"]
            },
            "StripeBillableActionListInput": {
                "kind": "object",
                "type": "StripeBillableActionListInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "stripeEventName"
                    }
                ]
            },
            "StripeBillableActionInput": {
                "kind": "object",
                "type": "StripeBillableActionInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "id",
                        "validation": [
                            {
                                "type": "isUuid"
                            },
                            {
                                "type": "unknown",
                                "constraints": [null]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "billableAction",
                        "validation": [
                            {
                                "type": "isNotEmpty"
                            },
                            {
                                "type": "unknown",
                                "constraints": [null]
                            }
                        ]
                    }
                ]
            },
            "StripeBillableActionCreateInput": {
                "kind": "object",
                "type": "StripeBillableActionCreateInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "billableAction",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 128]
                            },
                            {
                                "type": "isNotEmpty"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "description",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 255]
                            },
                            {
                                "type": "isNotEmpty"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "Float",
                        "required": true,
                        "name": "unitValue",
                        "validation": [
                            {
                                "type": "max",
                                "constraints": [1000]
                            },
                            {
                                "type": "min",
                                "constraints": [1]
                            },
                            {
                                "type": "isInt"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "stripeEventName",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 128]
                            },
                            {
                                "type": "isNotEmpty"
                            }
                        ]
                    }
                ]
            },
            "StripeBillableActionUpdateInput": {
                "kind": "object",
                "type": "StripeBillableActionUpdateInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "id",
                        "validation": [
                            {
                                "type": "isUuid"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "billableAction",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 128]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "description",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 255]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "stripeEventName",
                        "validation": [
                            {
                                "type": "isLength",
                                "constraints": [1, 128]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "Float",
                        "required": false,
                        "name": "unitValue",
                        "validation": [
                            {
                                "type": "max",
                                "constraints": [1000]
                            },
                            {
                                "type": "min",
                                "constraints": [1]
                            },
                            {
                                "type": "isInt"
                            }
                        ]
                    }
                ]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": ["SCALAR", "OBJECT", "INTERFACE", "UNION", "ENUM", "INPUT_OBJECT", "LIST", "NON_NULL"]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}
