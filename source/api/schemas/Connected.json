{
    "types": {
        "input": {
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "GridNodeInput": {
                "kind": "object",
                "type": "GridNodeInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "name"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "id"
                    }
                ]
            },
            "TaskState": {
                "kind": "enum",
                "type": "TaskState",
                "description": "The possible states of a task.",
                "values": ["Pending", "Assigned", "CheckedOut", "Running", "Succeeded", "Failed", "Cancelled", "Retry"]
            },
            "TaskOrigin": {
                "kind": "enum",
                "type": "TaskOrigin",
                "description": "Where the task originated from.",
                "values": ["System", "User"]
            },
            "Float": {
                "kind": "scalar",
                "type": "Float",
                "description": "The `Float` scalar type represents signed double-precision fractional values as specified by [IEEE 754](https://en.wikipedia.org/wiki/IEEE_floating_point)."
            },
            "AtlasProcedureType": {
                "kind": "enum",
                "type": "AtlasProcedureType",
                "description": "The type of procedure.",
                "values": ["PortScan", "NorpCheck"]
            },
            "JSON": {
                "kind": "scalar",
                "type": "JSON",
                "description": "The `JSON` scalar type represents JSON values as specified by [ECMA-404](http://www.ecma-international.org/publications/files/ECMA-ST/ECMA-404.pdf)."
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "TaskResultType": {
                "kind": "enum",
                "type": "TaskResultType",
                "description": "Task result type.",
                "values": ["Success", "Failure", "Error"]
            },
            "TaskInput": {
                "kind": "object",
                "type": "TaskInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "taskId",
                        "validation": [
                            {
                                "type": "isNotEmpty"
                            },
                            {
                                "type": "isString"
                            }
                        ]
                    }
                ]
            },
            "TaskGroupInput": {
                "kind": "object",
                "type": "TaskGroupInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "groupdId",
                        "validation": [
                            {
                                "type": "isNotEmpty"
                            },
                            {
                                "type": "isString"
                            }
                        ]
                    }
                ]
            },
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "OrderBy": {
                "kind": "object",
                "type": "OrderBy",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "key"
                    },
                    {
                        "kind": "enum",
                        "type": "OrderByDirection",
                        "required": false,
                        "name": "direction"
                    }
                ]
            },
            "OrderByDirection": {
                "kind": "enum",
                "type": "OrderByDirection",
                "description": "The order direction of a query",
                "values": ["Ascending", "Descending"]
            },
            "PaginationInputWithFilters": {
                "kind": "object",
                "type": "PaginationInputWithFilters",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    },
                    {
                        "kind": "object",
                        "type": "ColumnFilter",
                        "required": true,
                        "name": "filters"
                    }
                ]
            },
            "ColumnFilter": {
                "kind": "object",
                "type": "ColumnFilter",
                "fields": [
                    {
                        "kind": "enum",
                        "type": "ColumnFilterConditionOperator",
                        "required": true,
                        "name": "operator"
                    },
                    {
                        "kind": "scalar",
                        "type": "Boolean",
                        "required": false,
                        "name": "caseSensitive"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "column"
                    },
                    {
                        "kind": "scalar",
                        "type": "JSON",
                        "required": true,
                        "name": "value"
                    }
                ]
            },
            "ColumnFilterConditionOperator": {
                "kind": "enum",
                "type": "ColumnFilterConditionOperator",
                "description": "The operator of a field filter",
                "values": [
                    "Equal",
                    "NotEqual",
                    "GreaterThan",
                    "GreaterThanOrEqual",
                    "LessThan",
                    "LessThanOrEqual",
                    "Like",
                    "NotLike",
                    "In",
                    "NotIn",
                    "IsNull",
                    "IsNotNull"
                ]
            },
