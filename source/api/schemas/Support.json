{
    "types": {
        "input": {
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "SupportTicketStatus": {
                "kind": "enum",
                "type": "SupportTicketStatus",
                "description": "The status of a support ticket",
                "values": ["Open", "Closed", "Archived"]
            },
            "RichContentFormat": {
                "kind": "enum",
                "type": "RichContentFormat",
                "description": "The format of the string rich-content",
                "values": ["Markdown", "Html", "PlainText"]
            },
            "SupportTicketCommentSource": {
                "kind": "enum",
                "type": "SupportTicketCommentSource",
                "values": ["User", "Agent"]
            },
            "SupportTicketCommentVisibility": {
                "kind": "enum",
                "type": "SupportTicketCommentVisibility",
                "values": ["Public", "Internal"]
            },
            "MediaObjectType": {
                "kind": "enum",
                "type": "MediaObjectType",
                "values": ["Image", "Video", "File"]
            },
            "PaginationInput": {
                "kind": "object",
                "type": "PaginationInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    }
                ]
            },
            "SupportTicketCommentCreateInput": {
                "kind": "object",
                "type": "SupportTicketCommentCreateInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "ticketId",
                        "validation": [
                            {
                                "type": "isUuid"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "content"
                    },
                    {
                        "kind": "enum",
                        "type": "RichContentFormat",
                        "required": false,
                        "name": "contentType"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "replyToCommentId",
                        "validation": [
                            {
                                "type": "isUuid"
                            }
                        ]
                    },
                    {
                        "kind": "enum",
                        "type": "SupportTicketCommentVisibility",
                        "required": false,
                        "name": "visibility"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "emailAddress",
                        "validation": [
                            {
                                "type": "isEmail"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "emailName"
                    }
                ]
            },
            "SupportTicketCreateInput": {
                "kind": "object",
                "type": "SupportTicketCreateInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "title",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [256]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "description"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "type",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [36]
                            },
                            {
                                "type": "isIn",
                                "constraints": [["Contact", "SupportArticleFeedback"]]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "emailAddress",
                        "validation": [
                            {
                                "type": "isEmail"
                            }
                        ]
                    },
                    {
                        "kind": "object",
                        "type": "SupportTicketCommentCreateInput",
                        "required": false,
                        "name": "initialComment"
                    }
                ]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": ["SCALAR", "OBJECT", "INTERFACE", "UNION", "ENUM", "INPUT_OBJECT", "LIST", "NON_NULL"]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}
